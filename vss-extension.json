{
    "manifestVersion": 1,
    "id": "CycleTimeHistogramExtension",
    "publisher": "AltamirJuniorDias",
    "version": "1.0.1",
    "name": "Cycle Time Histogram",
    "description": "Cycle Time Histogram",
    "public": true,
    "icons": {
        "default": "images/icon.png"
    },
    "categories": ["Azure Boards"],
    "repository": {
        "type": "git",
        "uri": "https://github.com/altamir-junior-dias/azure-devops-extension-cycle-time-histogram"
      },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "CycleTime",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog",
                ".CycleTimeConfiguration"
            ],
            "properties": {
                "name": "Cycle Time",
                "description": "Cycle Time histogram with percentiles",
                "catalogIconUrl": "images/icon.png",
                "previewImageUrl": "images/icon.png",
                "uri": "widgets-cycle-time-widget.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 7
                    },
                    {
                        "rowSpan": 2,
                        "columnSpan": 8
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 7
                    },
                    {
                        "rowSpan": 3,
                        "columnSpan": 8
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 2
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 3
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 4
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 5
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 6
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 7
                    },
                    {
                        "rowSpan": 4,
                        "columnSpan": 8
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "CycleTimeConfiguration",
            "type": "ms.vss-dashboards-web.widget-configuration",
            "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
            "properties": {
                "name": "Cycle Time - Configuration",
                "description": "Cycle Time - Configuration",
                "uri": "widgets-cycle-time-configuration.html"
            }
        }
    ],

    "files": [
        { "path": "dist", "addressable": true, "packagePath": "/" },
        { "path": "images", "addressable": true, "packagePath": "/images" },

        { "path": "node_modules/vss-web-extension-sdk/lib", "addressable": true, "packagePath": "/lib" }
    ],
    "scopes": ["vso.work"]
}